{    
    "alpha":{
        "case":{
            "upper":{
                "code":[
                    "result = ''.join(random.choices(string.ascii_uppercase, k=num_of_digits))"
                ]
            },
            "lower":{
                "code":[
                    "result = ''.join(random.choices(string.ascii_lowercase, k=num_of_digits))"
                ]
            },
            "mixed":{
                "code":[
                    "result = ''.join(random.choices(string.ascii_uppercase + string.ascii_lowercase , k=num_of_digits))"
                ]
            }
        }
    },
    "numeric":{
        "code":[
            "result = ''.join(random.choices(string.digits , k=num_of_digits))"
        ]
    },
    "alphanumeric":{
        "code":[
            "result = ''.join(random.choices(string.ascii_uppercase + string.ascii_lowercase + string.digits, k=strlen))"
        ]
    }
}